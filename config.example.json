{
  "root": "https://echo.free.beeceptor.com",
  "headers": [
    {
      "key": "Accept-Language",
      "value": "en-US,en;q=0.5"
    }
  ],
  "endpoints": [
    {
      "name": "First Endpoint",
      "path": "/posts",
      "method": "GET",
      "headers": [
        {
          "key": "User-Agent",
          "value": "Mozilla/5.0 (X11; Linux x86_64; rv:140.0) Gecko/20100101 Firefox/140.0"
}
      ],
      "acceptStatus": [
        200
      ],
      "schema": "exampleSchemas/firstEndpoint.schema.json"
    },
    {
      "name": "Second Endpoint",
      "path": "/comments",
      "method": "POST",
      "body": "TEST BODY",
      "acceptStatus": [
        200
      ]
    }
  ]
}

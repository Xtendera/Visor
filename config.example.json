{
  "root": "https://echo.free.beeceptor.com",
  "saveResponseDir": "responses/",
  "headers": [
    {
      "key": "Accept-Language",
      "value": "en-US,en;q=0.5"
    }
  ],
  "jar": [
    {
      "name": "permanentcookie1",
      "value": "cookievalue1"
    }
  ],
  "endpoints": [
    {
      "name": "First Endpoint",
      "path": "/posts",
      "method": "GET",
      "headers": [
        {
          "key": "User-Agent",
          "value": "Mozilla/5.0 (X11; Linux x86_64; rv:140.0) Gecko/20100101 Firefox/140.0"
        }
      ],
      "cookies": [
        {
          "name": "tempcookie1",
          "value": "cookievalue1"
        }
      ],
      "jar": [
        {
          "name": "permanentcookie2",
          "value": "cookievalue2"
        }
      ],
      "acceptStatus": [
        200
      ],
      "schema": "example/schemas/firstEndpoint.schema.json"
    },
    {
      "name": "Second Endpoint",
      "path": "/cookies",
      "method": "GET",
      "bodyFile": "example/bodies/secondEndpoint.txt",
      "acceptStatus": [
        200
      ],
      "saveResponse": "responses/Second Custom Endpoint.json"
    }
  ]
}
